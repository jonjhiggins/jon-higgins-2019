{
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "extends": ["standard", "plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": { "graphql": true }
}
